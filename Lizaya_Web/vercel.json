{
  "platform": "python",
  "python":{
    "env": "3.9",
    "packages": [
      "flask",
      "fastapi",
      "bs4"],
    "settings": "https://github.com/josh-art/Lizaya-web/blob/main/Lizaya_Web/Lizaya_Web/settings.py"
  }
}
{
  "framework": "django"
}
{
  "public": true
}
{
  "builds": [
   { "src": "*.html", "use": "@vercel/static" },
   { "src": "*.py", "use": "@vercel/python" },
   { "src": "*.js", "use": "@vercel/node" }
  ]
 
}
{
  "routes": [
    {
      "src": "/home-page",
      "headers": { "cache-control" "s-maxage=1000" },
      "dest": "/index1.html"
    },
  ]
}
